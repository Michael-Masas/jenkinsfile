pipeline {
    agent any

    stages {
        stage('Print Job Name') {
            steps {
                echo env.JOB_NAME
            }
        }
        stage('Print Build Number') {
            steps {
                echo env.BUILD_NUMBER
            }
        }
        stage('Print Workspace') {
            steps {
                echo env.JENKINS_HOME
            }
        }
    }
}
